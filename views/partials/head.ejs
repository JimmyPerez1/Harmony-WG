<!DOCTYPE html>
<html lang="<%= (typeof lang !== 'undefined' && lang==='es') ? 'es' : 'en' %>">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!-- Styles -->
  <link rel="stylesheet" href="/css/main.css">
  <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />

  <!-- Title & Meta -->
  <title><%= (typeof pageTitle !== 'undefined' && pageTitle) ? pageTitle : t('meta.default.title') %></title>
  <meta name="description" content="<%= (typeof pageDescription !== 'undefined' && pageDescription) ? pageDescription : t('meta.default.description') %>">
  <meta name="keywords" content="Tampa clinic, rehabilitation Tampa, physical therapy Tampa, wellness center Tampa, Harmony Wellness Group">
  <meta name="author" content="Harmony Wellness Group">
  <link rel="canonical" href="https://harmonywellnessgroup.co/">

  <!-- Open Graph -->
  <meta property="og:title" content="Harmony Wellness Group | Rehabilitation & Wellness Clinic in Tampa, FL">
  <meta property="og:description" content="Evidence-based care in a supportive, healing environment. Request your appointment today.">
  <meta property="og:image" content="https://your-domain.com/harmony_logo.png">
  <meta property="og:url" content="https://your-domain.com/">
  <meta property="og:type" content="website">

  <script src="https://unpkg.com/lucide@latest"></script>

  <script type="application/ld+json">
  {
    "@context": "https://schema.org",
    "@type": "MedicalClinic",
    "name": "Harmony Wellness Group",
    "image": "https://your-domain.com/harmony_logo.png",
    "@id": "https://your-domain.com/",
    "url": "https://your-domain.com/",
    "telephone": "+1-813-555-1234",
    "address": {
      "@type": "PostalAddress",
      "streetAddress": "123 Main St",
      "addressLocality": "Tampa",
      "addressRegion": "FL",
      "postalCode": "33602",
      "addressCountry": "US"
    },
    "geo": {
      "@type": "GeoCoordinates",
      "latitude": 27.9506,
      "longitude": -82.4572
    },
    "priceRange": "$$"
  }
  </script>
</head>
